<h1>Store Locations: </h1>
<table>
	<thead>
		<tr>
			<th colspan="3"></th>
		</tr>
	</thead>

<tbody>
		<% @stores.each do |store| %>
			<tr>
				<td>
				<%= link_to store_path(store) do %>
					<%= store.address %> <%= store.street %> <%= store.zip %>
				<% end %>
				</td>
				<td><%= store.phone %></td>
				<td><%= link_to 'Edit', edit_store_path(store) %></td>
			</tr>
		<% end %>
	</tbody>
</table>

<br>

<%= link_to 'New Store', new_store_path %>
<%= link_to 'Products', products_path %>

